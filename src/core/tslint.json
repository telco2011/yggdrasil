{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "arrow-parens": false,
    "quotemark": [
      true,
      "single"
    ],
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": false,
    "max-line-length": [
      180
    ],
    "no-console": false,
    "trailing-comma": false
  },
  "rulesDirectory": []
}