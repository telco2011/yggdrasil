{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {
		"no-unused-expression": true
	},
	"rules": {
		"arrow-parens": false,
		"indent": [
			true,
			"tabs",
			4
		],
		"max-line-length": [
			180
		],
		"no-console": false,
		"object-literal-sort-keys": false,
		"ordered-imports": [
			false
		],
		"quotemark": [
			true,
			"single"
		],
		"trailing-comma": false
	},
	"rulesDirectory": []
}