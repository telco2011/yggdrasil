{
	"compilerOptions": {
		"allowJs": false,
		"baseUrl": ".",
		"declaration": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"module": "system",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"noLib": false,
		"noUnusedLocals": false,
		"outFile": "./lib/devs/yggdrasil.gulp.js",
		"removeComments": false,
		"rootDir": "./src",
		"sourceMap": false,
		"target": "es6"
	},
	"include": [
		"./src/devs/**/*.ts"
	]
}