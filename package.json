{
  "name": "yggdrasil",
  "version": "0.1.0",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean:lib && npm run compile",
    "clean:all": "npm run clean:lib && rm -rf logs node_modules",
    "clean:lib": "find ./lib -type f -name '*.js' -delete && find ./lib -type f -name '*.js.map' -delete && find ./lib -type f -name '*.ts' -delete && rm -f ./lib/common/tslint.json",
    "compile": "npm run tslint && npm run copy:tslint && tsc",
    "copy:tslint": "cp ./src/core/tslint.json ./lib/core",
    "tslint": "tslint -p tsconfig.json -c ./src/core/tslint.json -t verbose",
    "publish": "npm run build && lerna publish --skip-git"
  },
  "author": "David Lopez",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/express-session": "^1.15.6",
    "@types/mocha": "^2.2.46",
    "@types/mongoose": "^4.7.32",
    "@types/node": "^9.3.0",
    "@types/winston": "^2.3.7",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "lerna": "^2.7.0",
    "mocha": "^4.1.0",
    "mocha-typescript": "^1.1.12",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "chalk": "^2.3.0",
    "compression": "^1.7.1",
    "connect-mongo": "^2.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-list-routes": "^0.1.4",
    "express-session": "^1.15.6",
    "figlet": "^1.2.0",
    "lusca": "^1.5.2",
    "moment": "^2.20.1",
    "mongoose": "^4.13.9",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "q": "^1.5.1",
    "require-env": "^0.2.1",
    "rotating-file-stream": "^1.3.4",
    "winston": "^2.4.0"
  }
}
